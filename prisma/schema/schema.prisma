generator client {
  provider = "prisma-client"

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


cont model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  posts     Post[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}